{
  "name": "wunderpong",
  "version": "2.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test:server": "jest server",
    "predev": "echoascii dev && run-s helper:cleanup-uploads",
    "dev": "run-p dev:client dev:server",
    "dev:client": "parcel ./client/index.html --no-cache -d __dev__/client",
    "dev:server": "nodemon ./server",
    "build": "echoascii prod && run-s helper:cleanup-dist helper:cleanup-uploads build:client build:server",
    "build:client": "run-s bundle:client",
    "build:server": "run-s bundle:server helper:copy-model",
    "bundle:client": "rimraf dist/client && NODE_ENV=production parcel build ./client/index.html --no-source-maps --no-cache -d dist/client --public-url /",
    "bundle:server": "rimraf dist/server && NODE_ENV=production parcel build ./server/index.js --no-source-maps --no-cache -d dist/server -t node",
    "deploy": "run-s build helper:commit-files-to-heroku",
    "start": "node dist/server/index.js",
    "helper:cleanup-dist": "rimraf dist",
    "helper:cleanup-uploads": "rimraf uploads",
    "helper:commit-files-to-heroku": "git add dist && git add package.json && git add yarn.lock && git commit -m \"New release\" && git push heroku master",
    "helper:copy-model": "cp ./server/utils/model.json ./dist/server/"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "dist/*"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "deploy-heroku": "^0.0.0",
    "echoascii": "^1.0.3",
    "jest": "^23.1.0",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2",
    "parcel-bundler": "^1.7.1",
    "postcss-apply": "^0.10.0",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-calc": "^6.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-color-gray": "^4.1.0",
    "postcss-color-hex-alpha": "^3.0.0",
    "postcss-color-hsl": "^2.0.0",
    "postcss-color-hwb": "^3.0.0",
    "postcss-color-rgb": "^2.0.0",
    "postcss-color-rgba-fallback": "^3.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^7.0.0",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-for": "^2.1.1",
    "postcss-inject": "^1.0.1",
    "postcss-media-minmax": "^3.0.0",
    "postcss-modules": "^1.1.0",
    "postcss-nesting": "^5.0.0",
    "postcss-pseudo-class-any-link": "^4.0.0",
    "postcss-pseudoelements": "^5.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-selector-matches": "^3.0.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "arpad": "^1.0.2",
    "babel-register": "^6.26.0",
    "camelcase-keys": "^4.2.0",
    "classnames": "^2.2.6",
    "copy-text-to-clipboard": "^1.0.4",
    "cors": "^2.8.4",
    "date-fns": "^1.29.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "face-recognition": "^0.9.3",
    "inline-style-prefixer": "^4.0.0",
    "lodash.capitalize": "^4.2.1",
    "lodash.chunk": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.flatten": "^4.4.0",
    "lodash.flattendeep": "^4.4.0",
    "lodash.frompairs": "^4.0.1",
    "lodash.noop": "^3.0.1",
    "lodash.pick": "^4.4.0",
    "lodash.reverse": "^4.0.1",
    "lodash.set": "^4.3.2",
    "lodash.snakecase": "^4.1.1",
    "lodash.sortby": "^4.7.0",
    "lodash.throttle": "^4.1.1",
    "lodash.uniq": "^4.5.0",
    "lottie-web": "^5.3.1",
    "node-fetch": "^2.1.2",
    "pg": "^7.4.3",
    "pg-format": "^1.0.4",
    "prompt-confirm": "^2.0.4",
